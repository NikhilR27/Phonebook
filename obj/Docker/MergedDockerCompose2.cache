services:
  db:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: C1bDigital123
    image: mcr.microsoft.com/mssql/server:2017-latest
  phonebook.api:
    build:
      context: /Users/abnr355/Projects/PhoneBook
      dockerfile: PhoneBook.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: PhoneBook.API
      target: base
    container_name: PhoneBook.API
    depends_on:
      db:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: phonebook:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/PhoneBook.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - /Users/abnr355/Projects/PhoneBook/PhoneBook.API:/app:rw
    - /Users/abnr355/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/abnr355/.aspnet/https:/root/.aspnet/https:ro
    - /Users/abnr355/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /Users/abnr355/.nuget/packages:/root/.nuget/packages:ro
    - /Users/abnr355/Projects/PhoneBook:/src:rw
version: '3.4'